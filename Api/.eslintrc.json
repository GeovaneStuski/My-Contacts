{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
      "linebreak-style": "off",
      "class-methods-use-this": "off",
      "consistent-return": "off",
      "camelcase": "off",
      "no-plusplus": "off",
      "no-await-in-loop": "off"
    }
}
